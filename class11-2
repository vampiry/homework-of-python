#計算相似度
count = 0
i = 0
for a in seq:
    if seq[0][i] == seq[1][i] and seq[1][i] == seq[2][i] and a != '-':
        count += 100
    elif seq[0][i] == seq[1][i] and seq[1][i] != seq[2][i]:   
        count += 100*2/3
    elif seq[0][i] != seq[1][i] and seq[1][i] == seq[2][i]: 
        count + 100*2/3
    elif seq[0][i] != seq[1][i] and seq[0][i] == seq[2][i]:
        count += 100*2/3
    else:
        count += 0
    i = i + 1
print (count)

